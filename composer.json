{
    "name": "kuink/kuink",
    "description": "The Kuink Framework.",
    "keywords": [
        "framework",
        "kuink"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "vlucas/phpdotenv": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phan/phan": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "files": [
            "src/locallib.php"
        ],
        "classmap": [
            
        ],
        "psr-4": {
            "Kuink\\Bridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "scripts": {

    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}