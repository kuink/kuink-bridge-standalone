{
  "name": "kuink/kuink",
  "description": "The Kuink Framework.",
  "keywords": [
    "framework",
    "kuink"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "../kuink-core"
    },
    {
      "type": "path",
      "url": "../kuink-theme-adminlte"
    }
  ],
  "require": {
    "php": "^7.3",
    "vlucas/phpdotenv": "^2.2",
    "kuink/kuink-core": "*",
    "kuink/kuink-theme-adminlte": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phan/phan": "^1.1",
    "friendsofphp/php-cs-fixer": "^2.13"
  },
  "autoload": {
    "files": [
    ],
    "classmap": [
    ],
    "psr-4": {
      "Kuink\\Bridge\\": "src/",
      "Kuink\\Bridge\\Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}